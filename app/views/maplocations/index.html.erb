<p id="notice"><%= notice %></p>

<h1>Listing Maplocations</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maplocations.each do |maplocation| %>
      <tr>
        <td><%= maplocation.address %></td>
        <td><%= maplocation.latitude %></td>
        <td><%= maplocation.longitude %></td>
        <td><%= link_to 'Show', maplocation %></td>
        <td><%= link_to 'Edit', edit_maplocation_path(maplocation) %></td>
        <td><%= link_to 'Destroy', maplocation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Maplocation', new_maplocation_path %>
